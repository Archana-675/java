import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		String s = sc.next();
		int n= s.length();
		char ch[] = s.toCharArray();
		for(int i=0;i<=n;i++){
		    for(int j=i+1;j<=n;j++){
		        for(int k=0;k<j;k++){
		            System.out.println(s.charAt(k));
		        }
		        
		    }
		}
	}
}
